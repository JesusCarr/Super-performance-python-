#Fibonacci sequence with recursion, applying memoization.
#lru_cache from functools it's doing great!.

import time
from functools import lru_cache

@lru_cache(maxsize = 10000)

def fibonacci(n):  
    if n == 1:    
return 0  elif n == 2:   
return 1  elif n == 3:    
return 1    elif n > 3:    
return (fibonacci(n - 1) + fibonacci(n - 2))

for n in range(1, 501):  print(n, "  :   ", fibonacci(n))

%time
